<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="parentBox" spacing="5.0" xmlns:fx="http://javafx.com/fxml" fx:controller="asset.item.manage.ManageItemController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="%itemId"/>
    <TextField fx:id="idTextField"/>
    <Label text="%itemName" fx:id="nameLabel"/>
    <TextField fx:id="nameTextField"/>
    <Label text="%itemDimension" fx:id="dimensionLabel"/>
    <TextField fx:id="dimensionTextField"/>
    <Label text="%itemDescription" fx:id="descriptionLabel"/>
    <TextField fx:id="descriptionTextField"/>
    <Label text="%itemAmount"/>
    <TextField fx:id="amountTextField"/>
    <Label text="%itemUnit" fx:id="unitLabel"/>
    <TextField fx:id="unitTextField"/>
    <Label text="%itemPricePerUnit"/>
    <TextField fx:id="pricePerUnitTextField"/>
    <HBox fillHeight="false" spacing="10.0">
        <VBox.margin>
            <Insets top="20.0"/>
        </VBox.margin>
        <Button fx:id="cancelButton" maxWidth="1.7976931348623157E308"
                text="%cancel" HBox.hgrow="ALWAYS"/>
        <Button fx:id="saveButton" maxWidth="1.7976931348623157E308"
                text="%save" HBox.hgrow="ALWAYS"/>
    </HBox>
</VBox>
